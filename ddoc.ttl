@prefix accr: <http://publications.europa.eu/resource/authority/access-right/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ddoc: <https://w3id.org/emmo/application/datadoc#> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
#@prefix emmo: <https://w3id.org/emmo/hume#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix oteio: <https://w3id.org/emmo/domain/oteio#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix stat: <http://purl.org/adms/status/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/emmo/application/datadoc> a owl:Ontology ;
    dcterms:title "Data documentation ontology" ;
    vann:preferredNamespacePrefix "ddoc" ;
    vann:preferredNamespaceUri "https://w3id.org/emmo/application/datadoc#" ;
    owl:priorVersion "0.0.0" ;
    owl:versionIRI <https://w3id.org/emmo/application/datadoc/0.0.1> ;
    owl:versionInfo "0.0.1" .

eli:LegalResource a owl:Class ;
    skos:definition "This class represents the legislation, policy or policies that lie behind the Rules that govern the service." ;
    vann:usageNote "The definition and properties of the Legal Resource class are aligned with the ontology included in \"Council conclusions inviting the introduction of the European Legislation Identifier (ELI)\". For describing the attributes of a Legal Resource (labels, preferred labels, alternative labels, definition, etc.) we refer to the (ELI) ontology. In this data specification the use is restricted to instances of this class that follow the (ELI) URI guidelines." ;
    rdfs:subClassOf foaf:Document .

dcatap:applicableLegislation a owl:ObjectProperty ;
    skos:prefLabel "applicableLegislation" ;
    skos:definition "The legislation that mandates the creation or management of the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range eli:LegalResource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcatap:availability a owl:DatatypeProperty ;
    skos:prefLabel "availability" ;
    skos:definition "An indication how long it is planned to keep the Distribution of the Dataset available." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range xsd:dateTime ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcatap:status a owl:ObjectProperty ;
    skos:prefLabel "status" ;
    skos:definition "The status of the distribution in the context of maturity lifecycle." ;
    vann:usageNote "It MUST take one of the values: 'stat:Completed', 'stat:Deprecated', 'stat:UnderDevelopment', 'stat:Withdrawn'." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range skos:Concept ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

accr:CONFIDENTIAL a dcterms:RightsStatement ;
    vann:usageNote "Disclosure of this data could cause damage to interested parties such as public administrations and businesses. It may refer to personal and professional information as well as to information in the context of business, commerce or trade."@en ;
    rdfs:isDefinedBy <https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/access-right> ;
    skos:definition "Information that is not disclosed."@en ;
    skos:prefLabel "confidential"@en .

accr:NON_PUBLIC a dcterms:RightsStatement ;
    vann:usageNote "This category may include resources that contain sensitive or personal information."@en ;
    rdfs:isDefinedBy <https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/access-right> ;
    skos:definition "Not publicly accessible for privacy, security or other reasons."@en ;
    skos:prefLabel "non-public"@en .

accr:PUBLIC a dcterms:RightsStatement ;
    vann:usageNote "Permissible obstacles include registration and request for API keys, as long as anyone can request such registration and/or API keys."@en ;
    rdfs:isDefinedBy <https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/access-right> ;
    skos:definition "Publicly accessible by everyone."@en ;
    skos:prefLabel "public"@en .

accr:RESTRICTED a dcterms:RightsStatement ;
    vann:usageNote "This category may include resources that require payment, resources shared under non-disclosure agreements, resources for which the publisher or owner has not yet decided if they can be publicly released."@en ;
    rdfs:isDefinedBy <https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/access-right> ;
    skos:definition "Only available under certain conditions."@en ;
    skos:prefLabel "restricted"@en .

accr:SENSITIVE a dcterms:RightsStatement ;
    rdfs:isDefinedBy <https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/access-right> ;
    skos:definition "Sensitive non-classified (SNC) information, information whose unauthorised disclosure could cause damage to the Commission or other interested parties such as businesses, companies, intellectual property or personal data but which is not EU classified information."@en ;
    skos:prefLabel "sensitive"@en .

stat:Completed a skos:Concept ;
    skos:inScheme stat:1.0 ;
    skos:notation "Completed" ;
    skos:prefLabel "Completed"@en .

stat:Deprecated a skos:Concept ;
    skos:inScheme stat:1.0 ;
    skos:notation "Deprecated" ;
    skos:prefLabel "Deprecated"@en .

stat:UnderDevelopment a skos:Concept ;
    skos:inScheme stat:1.0 ;
    skos:notation "UnderDevelopment" ;
    skos:prefLabel "UnderDevelopment"@en .

stat:Withdrawn a skos:Concept ;
    skos:inScheme stat:1.0 ;
    skos:notation "Withdrawn" ;
    skos:prefLabel "Withdrawn"@en .

dcterms:LicenseDocument a owl:Class ;
    rdfs:comment "A legal document giving official permission to do something with a resource." .

dcterms:Location a owl:Class ;
    skos:definition "A spatial region or named place." ;
    vann:usageNote "It can be represented using a controlled vocabulary or with geographic coordinates. In the latter case, the use of the Core Location Vocabulary is recommended, following the approach described in the GeoDCAT-AP specification." .

dcterms:MediaType a owl:Class ;
    skos:definition "A media type, e.g. the format of a computer file." ;
    vann:usageNote "Media type instances follow the [IANA](https://www.w3.org/TR/vocab-dcat-3/#bib-iana-media-types) vocabulary using the <http://www.iana.org/assignments/media-types/> namespace. For example, the IRI of the media type `text/turtle` is <http://www.iana.org/assignments/media-types/text/turtle>." .

dcterms:Standard a owl:Class ;
    skos:definition "A standard or other specification to which a resource conforms." .


dcterms:abstract a owl:AnnotationProperty ;
    skos:prefLabel "abstract" ;
    skos:definition "A summary of the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range rdf:langString ;
    dcat:theme ddoc:datadoc .

dcterms:accessRights a owl:ObjectProperty ;
    skos:prefLabel "accessRights" ;
    skos:definition "Information about who can access the resource or an indication of its security status." ;
    vann:usageNote "Access Rights may include information regarding access or restrictions based on privacy, security, or other policies. The following preferred Rights Statement individuals are defined: `accr:PUBLIC`, `accr:NON_PUBLIC`, `accr:CONFIDENTIAL`, `accr:RESTRICTED`, `accr:SENSITIVE`" ;
    rdfs:domain dcat:Resource ;
    rdfs:range dcterms:RightsStatement ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:accrualPeriodicity a owl:ObjectProperty ;
    skos:prefLabel "frequency" ;
    skos:definition "The frequency at which the Dataset is updated." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcterms:Frequency ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:bibliographicCitation a owl:AnnotationProperty ;
    skos:prefLabel "bibliographicCitation" ;
    skos:definition "A bibliographic reference for the resource." ;
    vann:usageNote "Recommended practice is to include sufficient bibliographic detail to identify the resource as unambiguously as possible." ;
    rdfs:domain dcat:Resource ;
    dcat:theme ddoc:datadoc .

dcterms:conformsTo a owl:ObjectProperty ;
    skos:prefLabel "conformsTo" ;
    skos:definition "An implementing rule or other specification." ;
    rdfs:domain dcat:Resource ;
    rdfs:range dcterms:Standard ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:contributor a owl:ObjectProperty ;
    skos:prefLabel "contributor" ;
    skos:definition "An entity responsible for making contributions to the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range foaf:Agent ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:creator a owl:ObjectProperty ;
    skos:prefLabel "creator" ;
    skos:definition "An entity responsible for producing the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range foaf:Agent ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

skos:definition a owl:AnnotationProperty ;
    skos:prefLabel "description" ;
    skos:definition "A free-text account of the resource." ;
    vann:usageNote "This property can be repeated for parallel language versions of the description." ;
    rdfs:domain dcat:Resource ;
    rdfs:range rdf:langString ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:mandatory .

dcterms:format a owl:ObjectProperty ;
    skos:prefLabel "format" ;
    skos:definition "The file format of the Distribution." ;
    vann:usageNote "dcat:mediaType SHOULD be used if the type of the distribution is defined by [IANA](https://www.w3.org/TR/vocab-dcat-3/#bib-iana-media-types)." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range dcterms:MediaTypeOrExtent ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:hasPart a owl:ObjectProperty ;
    skos:prefLabel "hasPart" ;
    skos:definition "A related resource that is included either physically or logically in the described resource." ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:identifier a owl:AnnotationProperty ;
    skos:prefLabel "identifier" ;
    skos:definition "URI or other unique identifier of the resource being described." ;
    vann:usageNote "Recommended practice is to identify the resource by means of a string conforming to an identification system. Examples include International Standard Book Number (ISBN), Digital Object Identifier (DOI), and Uniform Resource Name (URN).  Persistent identifiers should be provided as HTTP URIs." ;
    rdfs:domain foaf:Agent ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:isReferencedBy a owl:ObjectProperty ;
    skos:prefLabel "isReferencedBy" ;
    skos:definition "A related resource, such as a publication, that references, cites, or otherwise points to the documented resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range rdfs:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:issued a owl:DatatypeProperty ;
    skos:prefLabel "releaseDate" ;
    skos:definition "The date of formal issuance (e.g., publication) of the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range xsd:dateTime ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:language a owl:DatatypeProperty ;
    skos:prefLabel "language" ;
    skos:definition "A language of the resource." ;
    vann:usageNote """Recommended practice is to use either a non-literal value
representing a language from a controlled vocabulary such as
ISO 639-2 or ISO 639-3, or a literal value consisting of an
IETF Best Current Practice 47
[[IETF-BCP47](https://tools.ietf.org/html/bcp47)] language
tag.

Example values: "en", "it", "no", "da"

This property can be repeated if the resource is expressed
with multiple languages.
""" ;
    rdfs:domain dcat:Resource ;
    rdfs:range xsd:string ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:license a owl:ObjectProperty ;
    skos:prefLabel "license" ;
    skos:definition "A licence under which the resource is made available." ;
    rdfs:domain dcat:Resource ;
    rdfs:range dcterms:LicenseDocument ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:modified a owl:DatatypeProperty ;
    skos:prefLabel "modificationDate" ;
    skos:definition "The most recent date on which the resource was changed or modified." ;
    rdfs:domain dcat:Resource ;
    rdfs:range xsd:dateTime ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:publisher a owl:ObjectProperty ;
    skos:prefLabel "publisher" ;
    skos:definition "Agent responsible for making the resource available." ;
    rdfs:domain dcat:Resource ;
    rdfs:range foaf:Agent ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcterms:relation a owl:ObjectProperty ;
    skos:prefLabel "relatedResource" ;
    skos:definition "A resource with an unspecified relationship to the cataloged resource." ;
    rdfs:domain dcat:Relationship ;
    rdfs:range rdfs:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:source a owl:ObjectProperty ;
    skos:prefLabel "source" ;
    skos:definition "A related Dataset from which the described Dataset is derived." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Dataset ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcterms:spatial a owl:ObjectProperty ;
    skos:prefLabel "geographicalCoverage" ;
    skos:definition "A geographic region that is covered by the dataset." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcterms:Location ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcterms:temporal a owl:ObjectProperty ;
    skos:prefLabel "temporalCoverage" ;
    skos:definition "A temporal period that the Dataset covers." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcterms:PeriodOfTime ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcterms:title a owl:AnnotationProperty ;
    skos:prefLabel "title" ;
    skos:definition "A name given to the resource." ;
    vann:usageNote "This property can be repeated for parallel language versions of the name." ;
    rdfs:domain dcat:Resource ;
    rdfs:range rdf:langString ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:mandatory .

dcterms:type a owl:ObjectProperty ;
    skos:prefLabel "type" ;
    skos:definition "A type of the resource." ;
    vann:usageNote "A recommended controlled vocabulary data-type is foreseen. For agents, the value should be chosen from [ADMS publisher type](https://raw.githubusercontent.com/SEMICeu/ADMS-AP/master/purl.org/ADMS_SKOS_v1.00.rdf)." ;
    rdfs:domain "dcterms:LicenseDocument" ;
    rdfs:range skos:Concept ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

vann:usageNote a owl:AnnotationProperty ;
    skos:prefLabel "usageNote" ;
    skos:definition "A reference that provides information on how this resource is to be used." ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdf:langString ;
    dcat:theme ddoc:datadoc .

spdx:Checksum a owl:Class ;
    skos:definition "A preferred label of the concept." .

spdx:ChecksumAlgorithm a owl:Class ;
    skos:definition "Algorithm for Checksums." ;
    vann:usageNote "This specification does not impose any additional requirements to properties for this entity." .

spdx:algorithm a owl:ObjectProperty ;
    skos:prefLabel "algorithm" ;
    skos:definition "The algorithm used to produce the subject Checksum." ;
    rdfs:domain spdx:Checksum ;
    rdfs:range spdx:ChecksumAlgorithm ;
    dcat:theme ddoc:datadoc .

spdx:checksum a owl:ObjectProperty ;
    skos:prefLabel "checksum" ;
    skos:definition "A mechanism that can be used to verify that the contents of a distribution have not changed." ;
    vann:usageNote "The checksum is related to the downloadURL." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range spdx:Checksum ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

spdx:checksumValue a owl:DatatypeProperty ;
    skos:prefLabel "checksumValue" ;
    skos:definition "A lower case hexadecimal encoded digest value produced using a specific algorithm." ;
    rdfs:domain spdx:Checksum ;
    rdfs:range xsd:hexBinary ;
    dcat:theme ddoc:datadoc .

vcard:Kind a owl:Class ;
    skos:definition "A description following the vCard specification, e.g. to provide telephone number and e-mail address for a contact point." ;
    vann:usageNote "Note that the class Kind is the parent class for the four explicit types of vCard (Individual, Organization, Location, Group)." .

vcard:hasAddress a owl:AnnotationProperty ;
    skos:prefLabel "hasAddress" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasCountryName a owl:AnnotationProperty ;
    skos:prefLabel "hasCountryName" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasEmail a owl:AnnotationProperty ;
    skos:prefLabel "hasEmail" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasFamilyName a owl:AnnotationProperty ;
    skos:prefLabel "hasFamilyName" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasGender a owl:AnnotationProperty ;
    skos:prefLabel "hasGender" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasGeo a owl:AnnotationProperty ;
    skos:prefLabel "hasGeo" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasGivenName a owl:AnnotationProperty ;
    skos:prefLabel "hasGivenName" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasHonorificPrefix a owl:AnnotationProperty ;
    skos:prefLabel "hasHonorificPrefix" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasHonorificSuffix a owl:AnnotationProperty ;
    skos:prefLabel "hasHonorificSuffix" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasInstantMessage a owl:AnnotationProperty ;
    skos:prefLabel "hasInstantMessage" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasKey a owl:AnnotationProperty ;
    skos:prefLabel "hasKey" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasLanguage a owl:AnnotationProperty ;
    skos:prefLabel "hasLanguage" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasLogo a owl:AnnotationProperty ;
    skos:prefLabel "hasLogo" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasMember a owl:AnnotationProperty ;
    skos:prefLabel "hasMember" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasName a owl:AnnotationProperty ;
    skos:prefLabel "hasName" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasNickname a owl:AnnotationProperty ;
    skos:prefLabel "hasNickname" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasNote a owl:AnnotationProperty ;
    skos:prefLabel "hasNote" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasOrganizationName a owl:AnnotationProperty ;
    skos:prefLabel "hasOrganizationName" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasOrganizationUnit a owl:AnnotationProperty ;
    skos:prefLabel "hasOrganizationUnit" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasPhoto a owl:AnnotationProperty ;
    skos:prefLabel "hasPhoto" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasPostalCode a owl:AnnotationProperty ;
    skos:prefLabel "hasPostalCode" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasRegion a owl:AnnotationProperty ;
    skos:prefLabel "hasRegion" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasStreetAddress a owl:AnnotationProperty ;
    skos:prefLabel "hasStreetAddress" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasTelephone a owl:AnnotationProperty ;
    skos:prefLabel "hasTelephone" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasUID a owl:AnnotationProperty ;
    skos:prefLabel "hasUID" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

vcard:hasURL a owl:AnnotationProperty ;
    skos:prefLabel "hasURL" ;
    rdfs:domain vcard:Kind ;
    dcat:theme ddoc:datadoc .

adms:identifier a owl:ObjectProperty ;
    skos:prefLabel "otherIdentifier" ;
    skos:definition "A secondary identifier of the Dataset" ;
    rdfs:domain dcat:Dataset ;
    rdfs:range adms:Identifier ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

adms:sample a owl:ObjectProperty ;
    skos:prefLabel "sample" ;
    skos:definition "A sample distribution of the dataset." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Distribution ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

adms:versionNotes a owl:AnnotationProperty ;
    skos:prefLabel "versionNotes" ;
    skos:definition "A description of the differences between this version and a previous version of the resource." ;
    vann:usageNote "This property can be repeated for parallel language versions of the version notes." ;
    rdfs:domain "dcat:Resource" ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:DataService a owl:Class ;
    skos:definition "A collection of operations that provides access to one or more datasets or data processing functions." ;
    rdfs:subClassOf dcat:Resource .


dcat:Dataset a owl:Class ;
    skos:definition "A collection of data, published or curated by an agent, and available for access or download in one or more representations." ;
    vann:usageNote "This class represents the actual dataset as published by the dataset provider. In cases where a distinction between the actual dataset and its entry in the catalog is necessary (because metadata such as modification date and maintainer might differ), the catalog record class can be used for the latter." ;
    rdfs:subClassOf dcat:Resource .

dcat:DatasetSeries a owl:Class ;
    skos:definition "A collection of datasets that are published separately, but share some characteristics that group them." ;
    rdfs:subClassOf dcat:Dataset .


dcat:Distribution a owl:Class ;
    skos:definition "A physical embodiment of the Dataset in a particular format." ;
    vann:usageNote "This represents a general availability of a dataset. It implies no information about the actual access method of the data, i.e., whether by direct download, API, or through a Web page. The use of dcat:downloadURL property indicates directly downloadable distributions." ;
    rdfs:subClassOf dcat:Resource .


dcat:Relationship a owl:Class ;
    skos:definition "An association class for attaching additional information to a relationship between DCAT Resources." .

dcat:accessService a owl:ObjectProperty ;
    skos:prefLabel "accessService" ;
    skos:definition "A data service that gives access to the distribution of the dataset." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range dcat:DataService ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:accessURL a owl:ObjectProperty ;
    skos:prefLabel "accessURL" ;
    skos:definition "A URL that gives access to a Distribution of the Dataset. E.g. landing page, feed, SPARQL endpoint. Use for all cases except a simple download link, in which case downloadURL is preferred." ;
    vann:usageNote "The resource at the access URL may contain information about how to get the Dataset." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range rdfs:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:mandatory .

dcat:bbox a owl:AnnotationProperty ;
    skos:prefLabel "bbox" ;
    skos:definition "The geographic bounding box of a resource." ;
    rdfs:domain dcterms:Location ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:byteSize a owl:DatatypeProperty ;
    skos:prefLabel "byteSize" ;
    skos:definition "The size of a Distribution in bytes." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range xsd:nonNegativeInteger ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:centroid a owl:AnnotationProperty ;
    skos:prefLabel "centroid" ;
    skos:definition "The geographic center (centroid) of a resource." ;
    rdfs:domain dcterms:Location ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:compressFormat a owl:ObjectProperty ;
    skos:prefLabel "compressFormat" ;
    skos:definition "The format of the file in which the data is contained in a compressed form, e.g. to reduce the size of the downloadable file." ;
    vann:usageNote "It SHOULD be expressed using a media type as defined in the official register of media types managed by [IANA](https://www.w3.org/TR/vocab-dcat-3/#bib-iana-media-types)." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range dcterms:MediaType ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:contactPoint a owl:ObjectProperty ;
    skos:prefLabel "contactPoint" ;
    skos:definition "Contact information that can be used for sending comments about the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range vcard:Kind ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:distribution a owl:ObjectProperty ;
    skos:prefLabel "distribution" ;
    skos:definition "An available distribution for the dataset." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Distribution ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:downloadURL a owl:DatatypeProperty ;
    skos:prefLabel "downloadURL" ;
    skos:definition "A URL that is a direct link to a downloadable file in a given format. The format is indicated by the distribution's 'dterms:format' and/or 'dcat:mediaType'." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range xsd:anyURI ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:endpointDescription a owl:ObjectProperty ;
    skos:prefLabel "endpointDescription" ;
    skos:definition "A description of the services available via the end-points, including their operations, parameters etc." ;
    rdfs:domain dcat:DataService ;
    rdfs:range rdfs:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:endpointURL a owl:DatatypeProperty ;
    skos:prefLabel "endpointURL" ;
    skos:definition "The root location or primary endpoint of the service (an IRI)." ;
    rdfs:domain dcat:DataService ;
    rdfs:range xsd:anyURI ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:mandatory .

dcat:hasRole a owl:ObjectProperty ;
    skos:prefLabel "hasRole" ;
    skos:definition "A function of an entity or agent with respect to another entity or resource." ;
    rdfs:domain dcat:Relationship ;
    rdfs:range dcat:Role ;
    dcat:theme ddoc:datadoc .

dcat:hasVersion a owl:ObjectProperty ;
    skos:prefLabel "hasVersion" ;
    skos:definition "A related resource that is a version, edition, or adaptation of the described resource." ;
    vann:usageNote "This property is intended for relating a non-versioned or abstract resource to several versioned resources, e.g., snapshots." ;
    rdfs:domain dcat:Resource ;
    rdfs:range rdfs:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:inSeries a owl:ObjectProperty ;
    skos:prefLabel "inSeries" ;
    skos:definition "A dataset series of which the dataset is part." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:DatasetSeries ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:keyword a owl:AnnotationProperty ;
    skos:prefLabel "keyword" ;
    skos:definition "A keyword or tag describing the resource." ;
    rdfs:range rdf:langString ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:landingPage a owl:ObjectProperty ;
    skos:prefLabel "landingPage" ;
    skos:definition "A web page that provides access to the resource and/or additional information (e.g. the distribution for a dataset)." ;
    vann:usageNote """It is intended to point to a landing page at the original
data provider, not to a page on a site of a third party,
such as an aggregator.

For distributions, if they are accessible only through a
landing page (i.e. direct download URLs are not known), then
the landing page link should be duplicated as accessURL on a
distribution.
""" ;
    rdfs:domain dcat:Resource ;
    rdfs:range foaf:Document ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:mediaType a owl:ObjectProperty ;
    skos:prefLabel "mediaType" ;
    skos:definition "The media type of the Distribution as defined in the official register of media types managed by [IANA](https://www.w3.org/TR/vocab-dcat-3/#bib-iana-media-types)." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range dcterms:MediaType ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:packageFormat a owl:ObjectProperty ;
    skos:prefLabel "packageFormat" ;
    skos:definition "The format of the file in which one or more data files are grouped together, e.g. to enable a set of related files to be downloaded together." ;
    vann:usageNote "It SHOULD be expressed using a media type as defined in the official register of media types managed by [IANA](https://www.w3.org/TR/vocab-dcat-3/#bib-iana-media-types) and prefixed with `https://www.iana.org/assignments/media-types/`." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range dcterms:MediaType ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:qualifiedRelation a owl:ObjectProperty ;
    skos:prefLabel "qualifiedRelation" ;
    skos:definition "A description of a relationship with another resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range dcat:Relationship ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:servesDataset a owl:ObjectProperty ;
    skos:prefLabel "servesDataset" ;
    skos:definition "This property refers to a collection of data that this data service can distribute." ;
    rdfs:domain dcat:DataService ;
    rdfs:range dcat:Dataset ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:spatialResolutionInMeters a owl:DatatypeProperty ;
    skos:prefLabel "spatialResolution" ;
    skos:definition "The minimum spatial separation resolvable in a dataset, measured in meters." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:decimal ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:temporalResolution a owl:DatatypeProperty ;
    skos:prefLabel "temporalResolution" ;
    skos:definition "The minimum time period resolvable in the dataset." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:duration ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

dcat:theme a owl:ObjectProperty ;
    skos:prefLabel "theme" ;
    skos:definition "A category of the resource.  A resource may be associated with multiple themes." ;
    vann:usageNote "The set of themes used to categorize the resources are organized in a skos:ConceptScheme, skos:Collection, owl:Ontology or similar, describing all the categories and their relations in the catalog." ;
    rdfs:domain dcat:Resource ;
    rdfs:range skos:Concept ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:recommended .

dcat:version a owl:AnnotationProperty ;
    skos:prefLabel "version" ;
    skos:definition "Version indicator (name or identifier) of a resource." ;
    vann:usageNote "DCAT does not prescribe how a version name / identifier should be specified, and refers for guidance to [DWBP]'s Best Practice 7: Provide a version indicator." ;
    rdfs:domain dcat:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

locn:Geometry a owl:Class ;
    skos:definition "The locn:Geometry class provides the means to identify a location as a point, line, polygon, etc. expressed using coordinates in some coordinate reference system." ;
    vann:usageNote "This specification does not impose any additional requirements to properties for this entity." .

locn:geometry a owl:ObjectProperty ;
    skos:prefLabel "geometry" ;
    skos:definition "The geographic center (centroid) of a resource." ;
    rdfs:domain dcterms:Location ;
    rdfs:range locn:Geometry ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

prov:Attribution a owl:Class ;
    skos:definition "An agent that has some form of responsibility for a resource, but which is not a publisher nor a creator of the resource." ;
    vann:usageNote "See [explanations and examples in W3C/DCAT](https://www.w3.org/TR/vocab-dcat-3/#qualified-attribution)." ;
    rdfs:subClassOf prov:Agent .


prov:qualifiedAttribution a owl:ObjectProperty ;
    skos:prefLabel "qualifiedAttribution" ;
    skos:definition "An Agent having some form of responsibility for the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range prov:Attribution ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

prov:wasDerivedFrom a owl:ObjectProperty ;
    skos:prefLabel "wasDerivedFrom" ;
    skos:definition "A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity." ;
    vann:usageNote "Relates a resource to another resource it was derived from." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Resource ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

prov:wasGeneratedBy a owl:ObjectProperty ;
    skos:prefLabel "wasGeneratedBy" ;
    skos:definition "An activity that generated, or provides the business context for, the creation of the dataset." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range prov:Activity ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

foaf:Agent a owl:Class ;
    skos:definition "An agent (eg. person, group, software or physical artifact)." ;
    vann:usageNote "A resource that acts or has the power to act." .
    #rdfs:subClassOf dcterms:Agent .

foaf:Document a owl:Class ;
    skos:definition "A document." ;
    vann:usageNote "The Document class represents those things which are, broadly conceived, 'documents'. E.g. images." .

foaf:name a owl:DatatypeProperty ;
    skos:prefLabel "name" ;
    skos:definition "A name of the agent." ;
    rdfs:domain foaf:Agent ;
    rdfs:range xsd:string ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:mandatory .

foaf:page a owl:ObjectProperty ;
    skos:prefLabel "documentation" ;
    skos:definition "A page or document about this resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range foaf:Document ;
    dcat:theme ddoc:datadoc ;
    ddoc:conformance ddoc:optional .

ddoc:conformance a owl:ObjectProperty ;
    skos:prefLabel "conformance"@en ;
    skos:definition "Whether the annotation is mandatory, recommended or optional."@en ;
    vann:usageNote "It MUST take one of the values 'ddoc:mandatory', 'ddoc:recommended' or 'ddoc:optional'."@en ;
    rdfs:domain dcat:Resource ;
    rdfs:isDefinedBy <https://w3id.org/emmo/application/datadoc> ;
    rdfs:range ddoc:ConformanceLevel .

ddoc:curationDate a owl:DatatypeProperty ;
    skos:prefLabel "curationDate" ;
    skos:definition "Date of curation." ;
    rdfs:domain dcat:Resource ;
    rdfs:range xsd:dateTime ;
    dcat:theme ddoc:datadoc .

ddoc:curator a owl:ObjectProperty ;
    skos:prefLabel "curator" ;
    skos:definition "The agent that curated the resource." ;
    vann:usageNote "Use `ddoc:curationDate` to refer to the date of curation." ;
    rdfs:domain dcat:Resource ;
    rdfs:range foaf:Agent ;
    dcat:theme ddoc:datadoc .

ddoc:datadoc a skos:Concept ;
    skos:prefLabel "datadoc"@en ;
    skos:definition "Basic data documentation."@en ;
    vann:usageNote "Intended to be used as a theme individual for basic data documentation."@en .

ddoc:datatype a owl:ObjectProperty ;
    skos:prefLabel "datatype"@en ;
    skos:definition "A datatype for annotations and data properties."@en ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Datatype .

ddoc:process a skos:Concept ;
    skos:prefLabel "process"@en ;
    skos:definition "Basic documentation of processes."@en ;
    vann:usageNote "Intended to be used as a theme individual for basic documentation of processes."@en .

ddoc:unitSymbol a owl:AnnotationProperty ;
    skos:prefLabel "unitSymbol"@en ;
    skos:definition "A SI unit associated to a property."@en ;
    vann:usageNote "The unit should be written with standard SI unit symbols.  Examples: As, μm, J, m/s²"@en ;
    rdfs:domain dcat:Resource ;
    rdfs:range xsd:string .

oteio:Generator a owl:Class ;
    skos:definition "A generator that can serialise an instance of a datamodel into a distribution." ;
    rdfs:subClassOf oteio:Parser .


oteio:Parser a owl:Class ;
    skos:definition "A parser that can parse a distribution into an instance of a datamodel." .

oteio:generator a owl:ObjectProperty ;
    skos:prefLabel "generator" ;
    skos:definition "A generator that can create the distribution." ;
    rdfs:domain dcat:Distribution ;
    rdfs:range oteio:Generator ;
    dcat:theme ddoc:datadoc .

oteio:generatorType a owl:AnnotationProperty ;
    skos:prefLabel "generatorType" ;
    skos:definition "Generator type. Ex: `application/vnd.dlite-generate`." ;
    rdfs:domain oteio:Generator ;
    dcat:theme ddoc:datadoc .

oteio:hasConfiguration a owl:DatatypeProperty ;
    skos:prefLabel "configuration" ;
    skos:definition "A JSON string with configurations specific to the parser or generator." ;
    rdfs:domain oteio:Parser ;
    rdfs:range rdf:JSON ;
    dcat:theme ddoc:datadoc .

oteio:hasDatamodel a owl:ObjectProperty ;
    skos:prefLabel "datamodel" ;
    skos:definition "URI of DLite datamodel for the dataset." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range oteio:Datamodel ;
    dcat:theme ddoc:datadoc .

oteio:hasDatamodelStorage a owl:DatatypeProperty ;
    skos:prefLabel "datamodelStorage" ;
    skos:definition "URL to DLite storage plugin where the datamodel is stored." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:anyURI ;
    dcat:theme ddoc:datadoc .

oteio:mapping a owl:DatatypeProperty ;
    skos:prefLabel "mappings" ;
    skos:definition "A list of subject-predicate-object triples mapping the datamodel to ontological concepts." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range rdf:JSON ;
    dcat:theme ddoc:datadoc .

oteio:mappingFormat a owl:AnnotationProperty ;
    skos:prefLabel "mappingFormat" ;
    skos:definition "File format for `mappingURL`. Defaults to \"turtle\"." ;
    rdfs:domain dcat:Dataset ;
    dcat:theme ddoc:datadoc .

oteio:mappingURL a owl:DatatypeProperty ;
    skos:prefLabel "mappingURL" ;
    skos:definition "URL to a document defining the mappings of the datamodel. The file format is given by `mappingFormat`. Defaults to turtle." ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:anyURI ;
    dcat:theme ddoc:datadoc .

oteio:parser a owl:ObjectProperty ;
    skos:prefLabel "parser" ;
    skos:definition "A parser that can parse the distribution." ;
    rdfs:domain dcat:DataService ;
    rdfs:range oteio:Parser ;
    dcat:theme ddoc:datadoc .

oteio:parserType a owl:AnnotationProperty ;
    skos:prefLabel "parserType" ;
    skos:definition "Parser type. Ex: `application/vnd.dlite-parse`." ;
    rdfs:domain oteio:Parser ;
    dcat:theme ddoc:datadoc .

oteio:statement a owl:DatatypeProperty ;
    skos:prefLabel "statements" ;
    skos:definition "A list of subject-predicate-object triples with additional RDF statements documenting the resource." ;
    rdfs:domain dcat:Resource ;
    rdfs:range rdf:JSON ;
    dcat:theme ddoc:datadoc .

<https://w3id.org/pink/ddoc> a owl:Ontology ;
    dcterms:title "Concepts defined in the ddoc vocabulary."@en ;
    owl:versionIRI <https://w3id.org/pink/0.0.1/ddoc> .

rdfs:Resource a owl:Class ;
    skos:definition "A generic resource defined by RDFS." .

ddoc:mandatory a ddoc:ConformanceLevel ;
    skos:prefLabel "mandatory"@en .

ddoc:optional a ddoc:ConformanceLevel ;
    skos:prefLabel "optional"@en .

ddoc:recommended a ddoc:ConformanceLevel ;
    skos:prefLabel "recommended"@en .

dcat:Resource a owl:Class ;
    skos:definition "Resource published or curated by an agent." .

ddoc:ConformanceLevel a owl:Class ;
    skos:prefLabel "ConformanceLevel"@en ;
    skos:definition "The class of conformance levels. Includes the following individuals: `ddoc:mandatory`, `ddoc:recommended` and `ddoc:optional`."@en ;
    owl:oneOf ( ddoc:mandatory ddoc:optional ddoc:recommended ) .

dcterms:RightsStatement a owl:Class ;
    skos:definition "A statement about the intellectual property rights (IPR) held in or over a resource, a legal document giving official permission to do something with a resource, or a statement about access rights." ;
    vann:usageNote "EU pre-defines the following individuals: accr:PUBLIC, accr:NON_PUBLIC, accr:CONFIDENTIAL, accr:RESTRICTED, accr:SENSITIVE." .

skos:Concept a owl:Class ;
    skos:definition "An idea or notion; a unit of thought." ;
    vann:usageNote "In DCAT-AP, a Concept is used to denote codes within a codelist. See [DCAT-AP](https://semiceu.github.io/DCAT-AP/releases/3.0.0/#controlled-vocs) for expectations and more detail." .
