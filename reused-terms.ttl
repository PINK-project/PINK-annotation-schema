@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix pink: <https://w3id.org/pink#> .

<https://w3id.org/pink/reused-terms> rdf:type owl:Ontology ;
    owl:versionIRI <https://w3id.org/pink/0.0.1/reused-terms> ;
    dcterms:abstract "Terms from standard vocabularies reused by the PINK Annotation Schema."@en .

dcat:hasVersion rdf:type owl:AnnotationProperty ;
	pink:conformance pink:mandatory .

dcat:landingPage rdf:type owl:ObjectProperty .
dcterms:accessRights rdf:type owl:ObjectProperty .
dcterms:creator rdf:type owl:ObjectProperty ;
	pink:conformance pink:mandatory .

dcterms:description rdf:type owl:AnnotationProperty ;
	pink:conformance pink:mandatory .
	
dcterms:identifier rdf:type owl:DataProperty ;
	pink:conformance pink:optional ;
        dcterms:description "Alternative identifier to the IRI"@en .	

dcterms:license rdf:type owl:ObjectProperty ;
	pink:conformance pink:mandatory .

dcterms:modified rdf:type owl:DataProperty ;
	pink:conformance pink:optional .

dcterms:title rdf:type owl:AnnotationProperty .
vann:usageNote rdf:type owl:AnnotationProperty .
