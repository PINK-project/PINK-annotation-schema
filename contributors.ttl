    dcterms:publisher <https://pink-project.eu/> ;
    dcterms:title "Contributors"@en ;
    owl:versionInfo "0.0.1" .


pink:hasORCID rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf dcterms:identifier ;
    skos:prefLabel "hasORCID"@en ;
    skos:definition "Relates an individual to an ORCID that identifies the individual."@en ;
    rdfs:domain foaf:Person ;
    rdfs:range xsd:anyURI .



