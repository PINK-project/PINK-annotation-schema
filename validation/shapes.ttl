@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix pink: <https://w3id.org/pink#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

pink:AdvancedMaterialShape a sh:NodeShape ;
    sh:targetClass pink:AdvancedMaterial .

pink:AttributionShape a sh:NodeShape ;
    sh:node pink:AgentShape ;
    sh:targetClass prov:Attribution .

pink:BooleanDataShape a sh:NodeShape ;
    sh:node pink:SymbolicDataShape ;
    sh:targetClass pink:BooleanData .

pink:ByteDataShape a sh:NodeShape ;
    sh:node pink:ShortDataShape ;
    sh:targetClass pink:ByteData .

pink:ComputationShape a sh:NodeShape ;
    sh:node pink:ProcessShape ;
    sh:property [ sh:class pink:Software ;
            sh:path pink:hasSoftware ] ;
    sh:targetClass pink:Computation .

pink:ComputationalToolShape a sh:NodeShape ;
    sh:node pink:SoftwareShape ;
    sh:targetClass pink:ComputationalTool .

pink:ConceptShape a sh:NodeShape ;
    sh:targetClass skos:Concept .

pink:DataServiceShape a sh:NodeShape ;
    sh:node pink:ResourceShape ;
    sh:property [ sh:class rdfs:Resource ;
            sh:path dcat:endpointDescription ],
        [ sh:class <https://w3id.org/emmo/domain/oteio#Parser> ;
            sh:path <https://w3id.org/emmo/domain/oteio#parser> ],
        [ sh:datatype xsd:anyURI ;
            sh:path dcat:endpointURL ],
        [ sh:class dcat:Dataset ;
            sh:path dcat:servesDataset ] ;
    sh:targetClass dcat:DataService .

pink:DatasetSeriesShape a sh:NodeShape ;
    sh:node pink:DatasetShape ;
    sh:targetClass dcat:DatasetSeries .

pink:DateDataShape a sh:NodeShape ;
    sh:node pink:SymbolicDataShape ;
    sh:targetClass pink:DateData .

pink:DateTimeDataShape a sh:NodeShape ;
    sh:node pink:SymbolicDataShape ;
    sh:targetClass pink:DateTimeData .

pink:DatumShape a sh:NodeShape ;
    sh:node pink:DataShape ;
    sh:targetClass pink:Datum .

pink:DescriptionShape a sh:NodeShape ;
    sh:node pink:SignShape ;
    sh:property [ sh:class prov:Entity ;
            sh:path pink:isDescriptionOf ] ;
    sh:targetClass pink:Description .

pink:DistributionShape a sh:NodeShape ;
    sh:node pink:ResourceShape ;
    sh:property [ sh:class rdfs:Resource ;
            sh:path dcat:accessURL ],
        [ sh:datatype xsd:nonNegativeInteger ;
            sh:path dcat:byteSize ],
        [ sh:class dcterms:MediaType ;
            sh:path dcat:compressFormat ],
        [ sh:class dcterms:MediaType ;
            sh:path dcat:mediaType ],
        [ sh:class skos:Concept ;
            sh:path <http://data.europa.eu/r5r/status> ],
        [ sh:datatype xsd:datetime ;
            sh:path <http://data.europa.eu/r5r/availability> ],
        [ sh:class dcterms:MediaType ;
            sh:path dcat:packageFormat ],
        [ sh:class <https://w3id.org/emmo/domain/oteio#Generator> ;
            sh:path <https://w3id.org/emmo/domain/oteio#generator> ],
        [ sh:class <http://spdx.org/rdf/terms#Checksum> ;
            sh:path <http://spdx.org/rdf/terms#checksum> ],
        [ sh:class dcat:DataService ;
            sh:path dcat:accessService ],
        [ sh:datatype xsd:anyURI ;
            sh:path dcat:downloadURL ],
        [ sh:class dcterms:MediaTypeOrExtent ;
            sh:path dcterms:format ] ;
    sh:targetClass dcat:Distribution .

pink:DoubleDataShape a sh:NodeShape ;
    sh:node pink:NumericalDataShape ;
    sh:targetClass pink:DoubleData .

pink:FloatDataShape a sh:NodeShape ;
    sh:node pink:NumericalDataShape ;
    sh:targetClass pink:FloatData .

pink:IndicatorShape a sh:NodeShape ;
    sh:node pink:PropertyShape ;
    sh:targetClass pink:Indicator .

pink:InformationShape a sh:NodeShape ;
    sh:node pink:DataShape,
        pink:SignShape ;
    sh:targetClass pink:Information .

pink:InterpreterShape a sh:NodeShape ;
    sh:node pink:AgentShape ;
    sh:targetClass pink:Interpreter .

pink:LicenseDocumentShape a sh:NodeShape ;
    sh:node pink:RightsStatementShape ;
    sh:targetClass dcterms:LicenseDocument .

pink:LocationShape a sh:NodeShape ;
    sh:property [ sh:class <http://www.w3.org/ns/locn#Geometry> ;
            sh:path <http://www.w3.org/ns/locn#geometry> ],
        [ sh:path dcat:centroid ],
        [ sh:path dcat:bbox ] ;
    sh:targetClass dcterms:Location .

pink:MaterialProcessShape a sh:NodeShape ;
    sh:node pink:ProcessShape ;
    sh:targetClass pink:MaterialProcess .

pink:MatterShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:targetClass pink:Matter .

pink:MeasurementShape a sh:NodeShape ;
    sh:node pink:ProcessShape ;
    sh:targetClass pink:Measurement .

pink:MediaTypeShape a sh:NodeShape ;
    sh:targetClass dcterms:MediaType .

pink:ModelShape a sh:NodeShape ;
    sh:node pink:SignShape ;
    sh:targetClass pink:Model .

pink:ParticipantShape a sh:NodeShape ;
    sh:node pink:AgentShape,
        pink:ObjectShape ;
    sh:targetClass pink:Participant .

pink:RelationshipShape a sh:NodeShape ;
    sh:property [ sh:class dcat:Role ;
            sh:path dcat:hasRole ],
        [ sh:class rdfs:Resource ;
            sh:path dcterms:relation ] ;
    sh:targetClass dcat:Relationship .

pink:ReliabilityDocumentShape a sh:NodeShape ;
    sh:node pink:DocumentShape ;
    sh:targetClass pink:ReliabilityDocument .

pink:SsbdDimensionShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:targetClass pink:SsbdDimension .

pink:StandardShape a sh:NodeShape ;
    sh:targetClass dcterms:Standard .

pink:TierLevelShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:targetClass pink:TierLevel .

pink:TimeDataShape a sh:NodeShape ;
    sh:node pink:SymbolicDataShape ;
    sh:targetClass pink:TimeData .

pink:ToxicityShape a sh:NodeShape ;
    sh:node pink:SignShape ;
    sh:targetClass pink:Toxicity .

pink:ActivityShape a sh:NodeShape ;
    sh:property [ sh:class prov:Entity ;
            sh:path pink:hasOutput ],
        [ sh:class prov:Entity ;
            sh:path pink:used ],
        [ sh:class prov:Entity ;
            sh:path prov:used ],
        [ sh:class prov:Agent ;
            sh:path prov:wasAssociatedWith ],
        [ sh:class prov:Agent ;
            sh:path pink:wasAssociatedWith ],
        [ sh:class prov:Entity ;
            sh:path pink:hasInput ],
        [ sh:class prov:Activity ;
            sh:path prov:wasInformedBy ],
        [ sh:class prov:Activity ;
            sh:path pink:wasInformedBy ] ;
    sh:targetClass prov:Activity .

pink:IntDataShape a sh:NodeShape ;
    sh:node pink:LongDataShape ;
    sh:targetClass pink:IntData .

pink:IntegerDataShape a sh:NodeShape ;
    sh:node pink:NumericalDataShape ;
    sh:targetClass pink:IntegerData .

pink:LongDataShape a sh:NodeShape ;
    sh:node pink:IntegerDataShape ;
    sh:targetClass pink:LongData .

pink:ObjectShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:targetClass pink:Object .

pink:PropertyShape a sh:NodeShape ;
    sh:node pink:SignShape ;
    sh:property [ sh:class prov:Entity ;
            sh:path pink:isPropertyOf ],
        [ sh:path pink:hasUnit ] ;
    sh:targetClass pink:Property .

pink:RightsStatementShape a sh:NodeShape ;
    sh:node pink:DocumentShape ;
    sh:targetClass dcterms:RightsStatement .

pink:ShortDataShape a sh:NodeShape ;
    sh:node pink:IntDataShape ;
    sh:targetClass pink:ShortData .

pink:SoftwareShape a sh:NodeShape ;
    sh:node pink:DataShape ;
    sh:property [ sh:datatype xsd:boolean ;
            sh:path pink:hasGUI ],
        [ sh:datatype xsd:boolean ;
            sh:path pink:hasAPI ],
        [ sh:class pink:Model ;
            sh:path pink:implementsModel ],
        [ sh:class pink:Software ;
            sh:path pink:priorRelease ] ;
    sh:targetClass pink:Software .

pink:DatasetShape a sh:NodeShape ;
    sh:node pink:DataShape,
        pink:DatasetShape,
        pink:ResourceShape ;
    sh:property [ sh:class <http://www.w3.org/ns/adms#Identifier> ;
            sh:path <http://www.w3.org/ns/adms#identifier> ],
        [ sh:class prov:Activity ;
            sh:path prov:wasGeneratedBy ],
        [ sh:class dcat:Dataset ;
            sh:path dcterms:source ],
        [ sh:datatype xsd:decimal ;
            sh:path dcat:spatialResolutionInMeters ],
        [ sh:class pink:Datum ;
            sh:path pink:hasDatum ],
        [ sh:class dcterms:PeriodOfTime ;
            sh:path dcterms:temporal ],
        [ sh:class dcat:Resource ;
            sh:path prov:wasDerivedFrom ],
        [ sh:datatype xsd:anyURI ;
            sh:path <https://w3id.org/emmo/domain/oteio#mappingURL> ],
        [ sh:class dcterms:Location ;
            sh:path dcterms:spatial ],
        [ sh:class dcat:Distribution ;
            sh:path dcat:distribution ],
        [ sh:datatype xsd:anyURI ;
            sh:path <https://w3id.org/emmo/domain/oteio#hasDatamodelStorage> ],
        [ sh:datatype xsd:duration ;
            sh:path dcat:temporalResolution ],
        [ sh:class rdf:JSON ;
            sh:path <https://w3id.org/emmo/domain/oteio#mapping> ],
        [ sh:class dcat:DatasetSeries ;
            sh:path dcat:inSeries ],
        [ sh:class pink:Datum ;
            sh:path pink:hasPart ],
        [ sh:class dcterms:Frequency ;
            sh:path dcterms:accrualPeriodicity ],
        [ sh:class dcat:Distribution ;
            sh:path <http://www.w3.org/ns/adms#sample> ],
        [ sh:class <https://w3id.org/emmo/domain/oteio#Datamodel> ;
            sh:path <https://w3id.org/emmo/domain/oteio#hasDatamodel> ],
        [ sh:path <https://w3id.org/emmo/domain/oteio#mappingFormat> ] ;
    sh:targetClass dcat:Dataset,
        pink:Dataset .

pink:DocumentShape a sh:NodeShape ;
    sh:node pink:DataShape ;
    sh:targetClass foaf:Document .

pink:NumericalDataShape a sh:NodeShape ;
    sh:node pink:SymbolicDataShape ;
    sh:targetClass pink:NumericalData .

pink:ProcessShape a sh:NodeShape ;
    sh:node pink:ActivityShape ;
    sh:targetClass pink:Process .

pink:ResourceShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:property [ sh:class rdfs:Resource ;
            sh:path dcterms:isReferencedBy ],
        [ sh:class rdf:JSON ;
            sh:path <https://w3id.org/emmo/domain/oteio#statement> ],
        [ sh:class foaf:Agent ;
            sh:path dcterms:publisher ],
        [ sh:path dcat:version ],
        [ sh:class <http://www.w3.org/2006/vcard/ns#Kind> ;
            sh:path dcat:contactPoint ],
        [ sh:class dcterms:RightsStatement ;
            sh:path dcterms:accessRights ],
        [ sh:datatype xsd:dateTime ;
            sh:path dcterms:issued ],
        [ sh:datatype xsd:string ;
            sh:path dcterms:language ],
        [ sh:class foaf:Agent ;
            sh:path dcterms:creator ],
        [ sh:class <https://w3id.org/emmo/application/datadoc#ConformanceLevel> ;
            sh:path <https://w3id.org/emmo/application/datadoc#conformance> ],
        [ sh:class dcterms:LicenseDocument ;
            sh:path dcterms:license ],
        [ sh:datatype rdf:langString ;
            sh:path dcterms:abstract ],
        [ sh:class foaf:Document ;
            sh:path dcat:landingPage ],
        [ sh:class <http://data.europa.eu/eli/ontology#LegalResource> ;
            sh:path <http://data.europa.eu/r5r/applicableLegislation> ],
        [ sh:class prov:Attribution ;
            sh:path prov:qualifiedAttribution ],
        [ sh:path dcterms:bibliographicCitation ],
        [ sh:class foaf:Document ;
            sh:path foaf:page ],
        [ sh:datatype rdf:langString ;
            sh:path dcterms:title ],
        [ sh:datatype rdf:langString ;
            sh:path dcat:keyword ],
        [ sh:class foaf:Document ;
            sh:path pink:documentation ],
        [ sh:class foaf:Agent ;
            sh:path dcterms:contributor ],
        [ sh:class foaf:Agent ;
            sh:path <https://w3id.org/emmo/application/datadoc#curator> ],
        [ sh:datatype xsd:datetime ;
            sh:path <https://w3id.org/emmo/application/datadoc#curationDate> ],
        [ sh:datatype rdf:langString ;
            sh:path skos:definition ],
        [ sh:class dcat:Relationship ;
            sh:path dcat:qualifiedRelation ],
        [ sh:class rdfs:Resource ;
            sh:path dcat:hasVersion ],
        [ sh:datatype xsd:datetime ;
            sh:path dcterms:modified ],
        [ sh:class skos:Concept ;
            sh:path dcat:theme ],
        [ sh:datatype xsd:string ;
            sh:path <https://w3id.org/emmo/application/datadoc#unitSymbol> ],
        [ sh:class dcterms:Standard ;
            sh:path dcterms:conformsTo ] ;
    sh:targetClass dcat:Resource .

pink:AgentShape a sh:NodeShape ;
    sh:node pink:AgentShape ;
    sh:property [ sh:path dcterms:identifier ],
        [ sh:datatype xsd:string ;
            sh:path foaf:name ],
        [ sh:class prov:Entity ;
            sh:path pink:attributed ] ;
    sh:targetClass prov:Agent,
        foaf:Agent .

pink:SignShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:property [ sh:class prov:Entity ;
            sh:path pink:standsFor ],
        [ sh:class pink:Interpreter ;
            sh:path pink:isInterpretedBy ] ;
    sh:targetClass pink:Sign .

pink:SymbolicDataShape a sh:NodeShape ;
    sh:node pink:DataShape ;
    sh:targetClass pink:SymbolicData .

pink:DataShape a sh:NodeShape ;
    sh:node pink:EntityShape ;
    sh:targetClass pink:Data .

pink:EntityShape a sh:NodeShape ;
    sh:property [ sh:class prov:Entity ;
            sh:path pink:wasDerivedBy ],
        [ sh:class pink:TierLevel ;
            sh:path pink:tierLevel ],
        [ sh:class pink:Matter ;
            sh:path pink:chemicalClass ],
        [ sh:class prov:Activity ;
            sh:path pink:wasGeneratedBy ],
        [ sh:class pink:Description ;
            sh:path pink:hasDescription ],
        [ sh:class pink:Property ;
            sh:path pink:hasProperty ],
        [ sh:class prov:Activity ;
            sh:path pink:participatesTo ],
        [ sh:class prov:Agent ;
            sh:path pink:wasAttributedTo ],
        [ sh:class pink:Sign ;
            sh:path pink:isRepresentedBy ],
        [ sh:class prov:Agent ;
            sh:path prov:wasAttributedTo ] ;
    sh:targetClass prov:Entity .

