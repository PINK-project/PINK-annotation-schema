@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pink: <https://w3id.org/pink#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

pink:AcidificationSubCategorised a owl:Class ;
    rdfs:label "Acidification" ;
    dcterms:description "Acidification of soil and water from atmospheric deposition of acidifying substances. Main sources are combustion processes emitting NOx and SOx. Contributes to forest decline and aquatic ecosystem damage." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "AcidificationSubCategorised" .

pink:AtmosphericEmissionsSubCategorised a owl:Class ;
    rdfs:label "Atmospheric Emissions" ;
    dcterms:description "Emissions released to air during production and life cycle. Includes particulate matter NOx SO2 CO2 VOCs and other substances contributing to air pollution and climate change." ;
    rdfs:subClassOf pink:EnvironmentalImpactPressuresCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "AtmosphericEmissionsSubCategorised" .

pink:AuthoritativeListsSubCategorised a owl:Class ;
    rdfs:label "Authoritative Lists" ;
    dcterms:description "Existing lists of problematic chemicals developed by government bodies or government-recognised expert bodies. Used extensively in hazard assessment as first step in selecting safe and sustainable chemicals. Includes substances of concern with particularly dangerous properties." ;
    rdfs:subClassOf pink:HazardousChemicalsCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "AuthoritativeListsSubCategorised" .

pink:BiodegradationPropertiesSubCategorised a owl:Class ;
    rdfs:label "Biodegradation Properties" ;
    dcterms:description "Ability of substances to biodegrade in different environmental compartments. Includes general biodegradability and specific assessment for marine and soil environments. Relevant for end-of-life impacts and persistence concerns." ;
    rdfs:subClassOf pink:BiodegradabilityCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "BiodegradationPropertiesSubCategorised" .

pink:CategorisedByFunctionalDimension a owl:Class ;
    rdfs:label "Functional Dimension" ;
    dcterms:description "The capability of chemicals and materials to perform their intended purpose and meet technical requirements. Includes performance characteristics quality attributes technical efficacy and fitness for intended applications throughout the life cycle." ;
    rdfs:subClassOf pink:SsbdCategorisedByDimension ;
    skos:prefLabel "CategorisedByFunctionalDimension" .

pink:CircularityFeaturesSubCategorised a owl:Class ;
    rdfs:label "Circularity Features" ;
    dcterms:description "Characteristics that enable circular economy including recyclability recycled content durability repairability and design for disassembly. Also includes technical parameters affecting recycling feasibility such as separation efficiency and energy requirements." ;
    rdfs:subClassOf pink:CircularityCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "CircularityFeaturesSubCategorised" .

pink:ClimateChangeSubCategorised a owl:Class ;
    rdfs:label "Climate Change" ;
    dcterms:description "Impacts related to increase in average global temperatures due to greenhouse gas emissions. Primarily caused by combustion of fossil fuels. Measured as global warming potential over specified time horizon." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "ClimateChangeSubCategorised" .

pink:CostComponentsSubCategorised a owl:Class ;
    rdfs:label "Cost Components" ;
    dcterms:description "Components of product cost including purchase costs production costs total production costs and minimum selling price. Important for assessing economic feasibility and market competitiveness of alternatives." ;
    rdfs:subClassOf pink:ProductCostCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "CostComponentsSubCategorised" .

pink:CustomerSafetyAndRightsSubCategorised a owl:Class ;
    rdfs:label "Customer Safety and Rights" ;
    dcterms:description "Protection of consumer health safety and rights. Includes ethical marketing natural substance content health and safety impacts and impacts on basic needs such as healthcare water food and shelter." ;
    rdfs:subClassOf pink:CustomerProtectionCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "CustomerSafetyAndRightsSubCategorised" .

pink:EcotoxicitySubCategorised a owl:Class ;
    rdfs:label "Ecotoxicity" ;
    dcterms:description "Potential toxic impacts on ecosystems which may damage individual species and ecosystem functioning. Includes accumulation in organisms. Assessed separately for freshwater terrestrial and marine environments." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "EcotoxicitySubCategorised" .

pink:EnergyMetricsSubCategorised a owl:Class ;
    rdfs:label "Energy Metrics" ;
    dcterms:description "Quantitative measures of energy consumption and efficiency. Includes total energy consumption energy efficiency ratios and cumulative energy demand across life cycle. Often used as proxy for overall environmental impact." ;
    rdfs:subClassOf pink:EnergyCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "EnergyMetricsSubCategorised" .

pink:EnvironmentalExposurePathwaysSubCategorised a owl:Class ;
    rdfs:label "Environmental Exposure Pathways" ;
    dcterms:description "Routes through which chemicals are released to the environment. Includes releases to water air and soil. Assessment considers fate distribution and potential exposures throughout the life cycle." ;
    rdfs:subClassOf pink:ExposureAssessmentCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "EnvironmentalExposurePathwaysSubCategorised" .

pink:EnvironmentalHazardsSubCategorised a owl:Class ;
    rdfs:label "Environmental Hazards" ;
    dcterms:description "Hazards that can cause adverse effects to the environment particularly aquatic organisms and ecosystems. Includes acute and chronic aquatic toxicity PBT/vPvB properties and ozone layer depletion. Based on CLP and REACH Annex XIII criteria." ;
    rdfs:subClassOf pink:HazardAssessmentCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "EnvironmentalHazardsSubCategorised" .

pink:EutrophicationSubCategorised a owl:Class ;
    rdfs:label "Eutrophication" ;
    dcterms:description "Over-enrichment of ecosystems with nutrients particularly nitrogen and phosphorus. Causes excessive growth of algae and plants disrupting original ecosystem balance. Affects terrestrial freshwater and marine environments differently." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "EutrophicationSubCategorised" .

pink:FinancialPerformanceSubCategorised a owl:Class ;
    rdfs:label "Financial Performance" ;
    dcterms:description "Financial performance metrics including profit net present value added value and payback period. Essential for assessing business viability and return on investment for sustainable alternatives." ;
    rdfs:subClassOf pink:ProfitabilityCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "FinancialPerformanceSubCategorised" .

pink:FullLifeCycleCostsSubCategorised a owl:Class ;
    rdfs:label "Full Life Cycle Costs" ;
    dcterms:description "Comprehensive costs across entire life cycle including life cycle cost externality costs and waste treatment costs. Accounts for environmental and social costs beyond direct financial costs to producer." ;
    rdfs:subClassOf pink:LifeCycleCostCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "FullLifeCycleCostsSubCategorised" .

pink:FundamentalRightsSubCategorised a owl:Class ;
    rdfs:label "Fundamental Rights" ;
    dcterms:description "Core human rights to be respected in operations and supply chain. Includes elimination of child and forced labour prevention of discrimination and harassment freedom of association access to basic needs and consideration of vulnerable groups." ;
    rdfs:subClassOf pink:HumanRightsCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "FundamentalRightsSubCategorised" .

pink:HumanExposurePathwaysSubCategorised a owl:Class ;
    rdfs:label "Human Exposure Pathways" ;
    dcterms:description "Routes through which humans can be exposed to chemicals. Includes dermal contact inhalation and ingestion. Exposure scenarios consider operational conditions and risk management measures for different life cycle stages." ;
    rdfs:subClassOf pink:ExposureAssessmentCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "HumanExposurePathwaysSubCategorised" .

pink:HumanHealthHazardsSubCategorised a owl:Class ;
    rdfs:label "Human Health Hazards" ;
    dcterms:description "Hazards that can cause adverse effects on human health. Includes acute toxicity skin and eye effects sensitisation mutagenicity carcinogenicity reproductive toxicity specific target organ toxicity and endocrine disruption. Based on CLP hazard classes." ;
    rdfs:subClassOf pink:HazardAssessmentCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "HumanHealthHazardsSubCategorised" .

pink:HumanToxicitySubCategorised a owl:Class ;
    rdfs:label "Human Toxicity" ;
    dcterms:description "Potential impacts on human health caused by absorbing substances from air water and soil via environmental pathways. Does not include direct product exposure. Distinguishes between cancer and non-cancer effects." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "HumanToxicitySubCategorised" .

pink:IntegratedImpactsSubCategorised a owl:Class ;
    rdfs:label "Integrated Impacts" ;
    dcterms:description "Aggregated impacts on areas of protection including human health damage ecosystem quality damage and resource depletion. Integrates multiple impact pathways to provide overall damage assessment. Includes biodiversity considerations." ;
    rdfs:subClassOf pink:EnvironmentalImpactEndpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "IntegratedImpactsSubCategorised" .

pink:IonisingRadiationSubCategorised a owl:Class ;
    rdfs:label "Ionising Radiation" ;
    dcterms:description "Impacts on human health from exposure to ionising radiation. Considers only routine releases under normal operating conditions not accidents. Measured relative to reference radioactive substance." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "IonisingRadiationSubCategorised" .

pink:MarketFactorsSubCategorised a owl:Class ;
    rdfs:label "Market Factors" ;
    dcterms:description "Market-related factors affecting adoption of sustainable chemicals. Includes stakeholder requirements product performance value-chain collaboration transparency of information and consumer willingness to pay premium for sustainability." ;
    rdfs:subClassOf pink:MarketCriteriaCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "MarketFactorsSubCategorised" .

pink:OzoneDepletionSubCategorised a owl:Class ;
    rdfs:label "Ozone Depletion" ;
    dcterms:description "Depletion of stratospheric ozone layer that protects from hazardous UV-B radiation. Caused by specific substances including CFCs and halons. Impacts include increased skin cancer and damage to plants." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "OzoneDepletionSubCategorised" .

pink:ParticulateMatterSubCategorised a owl:Class ;
    rdfs:label "Particulate Matter" ;
    dcterms:description "Adverse impacts on human health caused by emissions of particulate matter and precursors. Smaller particles are more dangerous as they penetrate deeper into lungs. Measured as mortality impact." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "ParticulateMatterSubCategorised" .

pink:PhotochemicalOzoneFormationSubCategorised a owl:Class ;
    rdfs:label "Photochemical Ozone Formation" ;
    dcterms:description "Formation of ground-level ozone that is harmful to humans plants and materials. Caused by photochemical reactions involving VOCs and NOx. Leads to summer smog and respiratory problems." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "PhotochemicalOzoneFormationSubCategorised" .

pink:PhysicalChemicalHazardsSubCategorised a owl:Class ;
    rdfs:label "Physical-Chemical Hazards" ;
    dcterms:description "Hazards related to physical and chemical properties of substances. Includes explosivity flammability oxidising properties corrosivity reactivity and instability. Based on CLP hazard classes for physical hazards." ;
    rdfs:subClassOf pink:HazardAssessmentCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "PhysicalChemicalHazardsSubCategorised" .

pink:ResourceEfficiencySubCategorised a owl:Class ;
    rdfs:label "Resource Efficiency" ;
    dcterms:description "Efficiency of resource utilization in production processes. Includes reaction efficiency atom economy E-factor and material intensity. Reflects green chemistry principles for minimizing waste and maximizing product yield." ;
    rdfs:subClassOf pink:ResourceAndProcessingCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "ResourceEfficiencySubCategorised" .

pink:ResourceQuantitySubCategorised a owl:Class ;
    rdfs:label "Resource Quantity" ;
    dcterms:description "Amount of resources consumed in production processes. Includes water consumption waste generation and net mass of materials consumed. Provides baseline for efficiency improvements and circularity strategies." ;
    rdfs:subClassOf pink:ResourceAndProcessingCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "ResourceQuantitySubCategorised" .

pink:ResourceTypeSubCategorised a owl:Class ;
    rdfs:label "Resource Type" ;
    dcterms:description "Origin and nature of resources used in production. Distinguishes between renewable and fossil feedstocks and identifies use of critical raw materials. Important for assessing sustainability of resource base." ;
    rdfs:subClassOf pink:ResourceAndProcessingCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "ResourceTypeSubCategorised" .

pink:ResourceUseSubCategorised a owl:Class ;
    rdfs:label "Resource Use" ;
    dcterms:description "Depletion of finite non-renewable resources including fossil fuels minerals and metals. Also considers water scarcity and land use impacts. Extracting high-quality resources today limits availability for future generations." ;
    rdfs:subClassOf pink:EnvironmentalImpactMidpointCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "ResourceUseSubCategorised" .

pink:SupplyChainManagementSubCategorised a owl:Class ;
    rdfs:label "Supply Chain Management" ;
    dcterms:description "Management of social and ethical aspects throughout supply chain. Includes tracking capacity corruption prevention job creation local employment and poverty prevention. Ensures responsible sourcing and supplier selection." ;
    rdfs:subClassOf pink:SsbdCategorisedBySubCategory,
        pink:SupplyChainResponsibilityCategorised ;
    skos:prefLabel "SupplyChainManagementSubCategorised" .

pink:WaterEmissionsSubCategorised a owl:Class ;
    rdfs:label "Water Emissions" ;
    dcterms:description "Emissions released to water bodies during production and life cycle. Includes total organic carbon and other pollutants affecting water quality. May be characterized by BOD and COD measurements." ;
    rdfs:subClassOf pink:EnvironmentalImpactPressuresCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "WaterEmissionsSubCategorised" .

pink:WorkerSafetySubCategorised a owl:Class ;
    rdfs:label "Worker Safety" ;
    dcterms:description "Safety and health protection for workers beyond direct chemical exposure. Includes compliance with standards noise reduction injury rates equipment safety and exposure time management. Complementary to safety dimension hazard assessment." ;
    rdfs:subClassOf pink:OccupationalHealthAndSafetyCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "WorkerSafetySubCategorised" .

pink:WorkingConditionsSubCategorised a owl:Class ;
    rdfs:label "Working Conditions" ;
    dcterms:description "Conditions under which work is performed. Includes working hours work-life balance fair wages training opportunities and availability of flexible work arrangements. Essential for decent work throughout value chain." ;
    rdfs:subClassOf pink:LabourRightsCategorised,
        pink:SsbdCategorisedBySubCategory ;
    skos:prefLabel "WorkingConditionsSubCategorised" .

<https://w3id.org/pink/ssbd-dimensions> a owl:Ontology ;
    dcterms:title "Categorisation of SSbD dimensions, categories and sub-categories."@en ;
    owl:versionIRI <https://w3id.org/pink/0.0.1/ssbd-dimensions> .

pink:BiodegradabilityCategorised a owl:Class ;
    rdfs:label "Biodegradability" ;
    dcterms:description "The ability of a material to decompose after interactions with biological elements. Via biological degradation living organisms can break down organic materials into substances such as water carbon dioxide and methane." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "BiodegradabilityCategorised" .

pink:CircularityCategorised a owl:Class ;
    rdfs:label "Circularity" ;
    dcterms:description "Guiding principle behind value-retention processes including reduce reuse repair refurbish remanufacture and recycle. Creating value retention loops at the design stage to minimize use of virgin resources and creation of waste." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "CircularityCategorised" .

pink:CustomerProtectionCategorised a owl:Class ;
    rdfs:label "Customer Protection" ;
    dcterms:description "Practice of safeguarding consumers of goods and services. Includes health and safety aspects feedback mechanisms privacy transparency and end of life responsibility for products containing chemicals." ;
    rdfs:subClassOf pink:CategorisedBySocialDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "CustomerProtectionCategorised" .

pink:EnergyCategorised a owl:Class ;
    rdfs:label "Energy" ;
    dcterms:description "Energy aspects related to production and life cycle of chemicals and materials. Energy generation and consumption represents main cause of environmental impacts such as air pollution and climate change." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "EnergyCategorised" .

pink:EnvironmentalImpactEndpointCategorised a owl:Class ;
    rdfs:label "Environmental Impact - Endpoint" ;
    dcterms:description "Characterisation at the level of Areas of Protection including natural ecosystems human health and resource availability. Endpoint indicators measure potential damage to these protection areas." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "EnvironmentalImpactEndpointCategorised" .

pink:HazardousChemicalsCategorised a owl:Class ;
    rdfs:label "Restricted and Hazardous Chemicals" ;
    dcterms:description "Lists of substances with particularly dangerous properties for human health and the environment that are already regulated or of concern. Includes authoritative lists developed by government bodies or government-recognised expert bodies." ;
    rdfs:subClassOf pink:CategorisedBySafetyDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "HazardousChemicalsCategorised" .

pink:HumanRightsCategorised a owl:Class ;
    rdfs:label "Human Rights" ;
    dcterms:description "Fundamental rights inherent to all human beings. Includes prevention of child and forced labour discrimination harassment and ensuring freedom of association access to basic needs and fair treatment." ;
    rdfs:subClassOf pink:CategorisedBySocialDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "HumanRightsCategorised" .

pink:LabourRightsCategorised a owl:Class ;
    rdfs:label "Labour Rights" ;
    dcterms:description "Legal rights and human rights related to labour conditions. Includes working hours wages work-life balance training opportunities and decent working conditions throughout the value chain." ;
    rdfs:subClassOf pink:CategorisedBySocialDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "LabourRightsCategorised" .

pink:LifeCycleCostCategorised a owl:Class ;
    rdfs:label "Life Cycle Cost" ;
    dcterms:description "Total cost of an asset over its life cycle including initial capital costs maintenance costs operating costs and residual value at end of life. May include externality costs associated with environmental and social impacts." ;
    rdfs:subClassOf pink:CategorisedByEconomicDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "LifeCycleCostCategorised" .

pink:MarketCriteriaCategorised a owl:Class ;
    rdfs:label "Market Criteria" ;
    dcterms:description "Market-related aspects for chemicals and materials including stakeholder requirements product performance value-chain collaboration transparency and willingness to pay for sustainable alternatives." ;
    rdfs:subClassOf pink:CategorisedByEconomicDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "MarketCriteriaCategorised" .

pink:OccupationalHealthAndSafetyCategorised a owl:Class ;
    rdfs:label "Occupational Health and Safety" ;
    dcterms:description "Protection of safety health and welfare of people engaged in work or employment. Includes prevention of work-related injuries and diseases and provision of safe and healthy working conditions." ;
    rdfs:subClassOf pink:CategorisedBySocialDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "OccupationalHealthAndSafetyCategorised" .

pink:ProductCostCategorised a owl:Class ;
    rdfs:label "Product Cost" ;
    dcterms:description "Costs incurred to produce a product including direct labour purchase of consumables and factory overhead. Also includes costs related to bringing product to market." ;
    rdfs:subClassOf pink:CategorisedByEconomicDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "ProductCostCategorised" .

pink:ProfitabilityCategorised a owl:Class ;
    rdfs:label "Profitability" ;
    dcterms:description "Measure of owner's major financial interest in income-formation process. Balance between generation of income and distribution of income for market products. Includes various established metrics for measuring profit." ;
    rdfs:subClassOf pink:CategorisedByEconomicDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "ProfitabilityCategorised" .

pink:SupplyChainResponsibilityCategorised a owl:Class ;
    rdfs:label "Supply Chain Responsibility" ;
    dcterms:description "Sourcing approach followed by companies to procure products and materials in an ethical sustainable and socially conscious way. Includes due diligence processes to identify prevent and mitigate adverse impacts in operations and supply chain." ;
    rdfs:subClassOf pink:CategorisedBySocialDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "SupplyChainResponsibilityCategorised" .

pink:EnvironmentalImpactPressuresCategorised a owl:Class ;
    rdfs:label "Environmental Impact - Pressures" ;
    dcterms:description "Quantified causes of changes to the environment. Pressures are emissions to air water and soil as well as resource use resulting from human activity including raw materials chemical processes and product characteristics." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "EnvironmentalImpactPressuresCategorised" .

pink:ExposureAssessmentCategorised a owl:Class ;
    rdfs:label "Exposure Assessment" ;
    dcterms:description "Assessment of contact of an organism with a chemical radiological or physical agent. Includes development of exposure scenarios and exposure estimations for different pathways and life cycle stages." ;
    rdfs:subClassOf pink:CategorisedBySafetyDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "ExposureAssessmentCategorised" .

pink:CategorisedBySafetyDimension a owl:Class ;
    rdfs:label "Safety Dimension" ;
    dcterms:description "The responsibility of protecting from harm or other psychophysical dangers. Refers to environmental and health safety (EHS) aspects of chemicals and materials throughout their life cycle." ;
    rdfs:subClassOf pink:SsbdCategorisedByDimension ;
    skos:prefLabel "CategorisedBySafetyDimension" .

pink:HazardAssessmentCategorised a owl:Class ;
    rdfs:label "Hazard Assessment" ;
    dcterms:description "Assessment of intrinsic properties and set of properties that make a substance dangerous or which has potential to cause adverse effects to living organisms or the environment. Includes human health environmental and physical-chemical hazards." ;
    rdfs:subClassOf pink:CategorisedBySafetyDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "HazardAssessmentCategorised" .

pink:ResourceAndProcessingCategorised a owl:Class ;
    rdfs:label "Resources and Processing" ;
    dcterms:description "Aspects that relate to and characterise the chemical/material production process including type and quantity of resource input as well as efficiency in their use. Includes considerations of feedstock type resource consumption and process efficiency." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "ResourceAndProcessingCategorised" .

pink:CategorisedByEconomicDimension a owl:Class ;
    rdfs:label "Economic Dimension" ;
    dcterms:description "The responsibility of achieving economic growth with a strategy to achieve sustainable development in the current and future resource-constrained world." ;
    rdfs:subClassOf pink:SsbdCategorisedByDimension ;
    skos:prefLabel "CategorisedByEconomicDimension" .

pink:CategorisedBySocialDimension a owl:Class ;
    rdfs:label "Social Dimension" ;
    dcterms:description "The responsibility of managing business in a way that respects protects and fulfils human rights of employees workers in the value chain customers and local communities." ;
    rdfs:subClassOf pink:SsbdCategorisedByDimension ;
    skos:prefLabel "CategorisedBySocialDimension" .

pink:SsbdCategorisedByDimension a owl:Class ;
    rdfs:label "SSbD Categorized by Dimension" ;
    dcterms:description "The superclass of all individuals categorised by SSbD dimension." ;
    skos:prefLabel "SsbdCategorisedByDimension" .

pink:CategorisedByEnvironmentalDimension a owl:Class ;
    rdfs:label "Environmental Dimension" ;
    dcterms:description "A condition of balance and interconnectedness that allows human society to satisfy its needs while neither exceeding the capacity of supporting ecosystems nor diminishing biological diversity." ;
    rdfs:subClassOf pink:SsbdCategorisedByDimension ;
    skos:prefLabel "CategorisedByEnvironmentalDimension" .

pink:EnvironmentalImpactMidpointCategorised a owl:Class ;
    rdfs:label "Environmental Impact - Midpoint" ;
    dcterms:description "Characterisation of environmental interventions at the level of cause-effect chain. Midpoint indicators compare environmental interventions at level between emissions/resource consumption and endpoint damage to ecosystems and human health." ;
    rdfs:subClassOf pink:CategorisedByEnvironmentalDimension,
        pink:SsbdCategorisedByCategory ;
    skos:prefLabel "EnvironmentalImpactMidpointCategorised" .

pink:SsbdCategorisedByCategory a owl:Class ;
    rdfs:label "SSbD Categorized by Catetory" ;
    dcterms:description "The superclass of all individuals categorised by SSbD category." ;
    rdfs:subClassOf pink:SsbdCategorisedByDimension ;
    skos:prefLabel "SsbdCategorisedByCategory" .

pink:SsbdCategorisedBySubCategory a owl:Class ;
    rdfs:label "SSbD Categorized by SubCategory" ;
    dcterms:description "The superclass of all individuals categorised by SSbD subcategory." ;
    rdfs:subClassOf pink:SsbdCategorisedByCategory ;
