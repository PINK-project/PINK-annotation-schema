@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pink: <https://w3id.org/pink#> .
@prefix emmo: <https://w3id.org/emmo/hume#> .
@prefix micro: <https://w3id.org/emmo/domain/microstructure#> .
@prefix cheminf: <http://semanticscience.org/resource/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .


<https://w3id.org/pink/cheminf-mappings> rdf:type owl:Ontology ;
    owl:versionIRI <https://w3id.org/pink/0.0.1/cheminf-mappings> ;
    owl:imports
        <https://w3id.org/pink/0.0.1> ,
        <https://w3id.org/pink/0.0.1/cheminf> ;
    dcterms:abstract "Mapping CHEMINF to the PINK Annotation Schema."@en ;
    dcterms:title "CHEMINF mappings"@en .


# General mappings
emmo:Property owl:subClassOf pink:Property .  # property
cheminf:CHEMINF_000025 owl:subClassOf pink:Indicator .  # physical descriptor
cheminf:CHEMINF_000266 owl:subClassOf emmo:ChemicalSubstance .  # chemical substance
obo:IAO_0000136 owl:subClassOf pink:isPropertyOf .  # is about


# End point-specific mappings (optional)
cheminf:CHEMINF_000256 owl:subClassOf micro:MeltingPointTemperature .  # melting point descriptor (more generally, it could be mapped to emmo:ThermodynamicTemperature)
