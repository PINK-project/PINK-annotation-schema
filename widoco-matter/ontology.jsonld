}, {
  "@id" : "https://w3id.org/emmo/hume#AmorphousMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByStructure"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "NonCrystallineMaterial"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A solid lacking a repeating, ordered atomic structure (long-range order)."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "AmorphousMaterial"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "A amorphous material resembles a frozen liquid with atoms arranged randomly, unlike crystalline solids with periodic patterns, leading to properties like no sharp melting point, irregular shapes, and isotropic characteristics (uniform properties in all directions)."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#ArtificialMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByOrigin"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Any substance engineered or created by humans, often to mimic or improve upon natural materials."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "ArtificialMaterial"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "Artificial materials are typically engineered through chemical synthesis from natural raw materials like oil or coal, resulting in polymers such as plastics, nylon, or polyester, designed for specific, enhanced properties like strength, durability, or moldability not found in nature."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#Atom",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MolecularEntity"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "ChemicalElement"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A standalone atom has direct part one 'nucleus' and one 'electron_cloud'.\n\nAn O 'atom' within an O₂ 'molecule' is an 'e-bonded_atom'.\n\nIn this material branch, H atom is a particular case, with respect to higher atomic number atoms, since as soon as it shares its electron it has no nucleus entangled electron cloud.\n\nWe cannot say that H₂ molecule has direct part two H atoms, but has direct part two H nucleus."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "Atom"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "An 'atom' is a 'nucleus' surrounded by an 'electron_cloud', i.e. a quantum system made of one or more bounded electrons."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#BondedAtom",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#Atom"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A bonded atom that shares at least one electron to the atom-based entity of which is part of."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "BondedAtom"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "A real bond between atoms is always something hybrid between covalent, metallic and ionic.\n\nIn general, metallic and ionic bonds have atoms sharing electrons."
  }, {
    "@language" : "en",
    "@value" : "The bond types that are covered by this definition are the strong electronic bonds: covalent, metallic and ionic."
  }, {
    "@language" : "en",
    "@value" : "This class can be used to represent molecules as simplified quantum systems, in which outer molecule shared electrons are un-entangled with the inner shells of the atoms composing the molecule."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#CeramicMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByType"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "Ceramic"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "An inorganic non-metallic solid made from elements (metals, non-metals, semi-metals like silicon) combined into compounds (oxides, carbides, nitrides) that are typically hard, strong, brittle, corrosion-resistant, and good insulators."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "CeramicMaterial"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "Ceramic materials are hardened by intense heat and typically used from pottery and tiles to advanced electronics and aerospace parts."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#ChemicalCompound",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@language" : "en",
    "@value" : "https://en.wikipedia.org/wiki/Chemical_compound"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#ChemicalSubstance"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A chemical substance composed of many identical molecules (or molecular entities) composed of atoms from more than one element held together by chemical bonds."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "ChemicalCompound"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#ChemicalEntity",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The union of ChemicalSubstance and MolecularEntity."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "ChemicalEntity"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "A chemical entity comprises the two different ways to represents matter: as single recognizable particle entity (molecular entity) and as a composition of particle entities (substance).\n\nThis distinction is not well assessed in actual chemical nomenclature, in which an element name refers to both the pure elemental substance or the atom.\n\nIn the EMMO we force the adoption of a more strict categorization based on mereotopology.\n\nThe class Material hosts the subclasses for which a substance can be identified without necessarily considering its nature of molecule/atom or substance (e.g. hydrocarbon is the class of both hydrocarbon molecules or gases)."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#ChemicalSubstance",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@language" : "en",
    "@value" : "https://doi.org/10.1351/goldbook.C01039"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#ChemicalEntity"
  }, {
    "@id" : "https://w3id.org/emmo/hume#Substance"
  } ],
  "http://www.w3.org/2002/07/owl#disjointWith" : [ {
    "@id" : "https://w3id.org/emmo/hume#MolecularEntity"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Matter of constant composition best characterized by the entities (molecules, formula units, atoms) it is composed of."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "ChemicalSubstance"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "A chemical substance is always composed of more than one molecular entity. It lays in the continuum or mesoscopic domain."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#CompactMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByCompactness"
  }, {
    "@id" : "https://w3id.org/emmo/hume#Solid"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A solid material whose particles are closely and firmly packed together, resulting in a high density, solid structure without significant internal voids or cavities."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "CompactMaterial"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#CompositeMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByType"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "Composite"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A material that contains two or more constituent materials."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "CompositeMaterial"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#CondensedMatter",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#StateOfMatter"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Solid or liquid matter."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "CondensedMatter"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "The subject of condensed matter physics that deals with the macroscopic and microscopic physical properties of matter, especially the solid and liquid phases which arise from electromagnetic forces between atoms. More generally, the subject deals with \"condensed\" phases of matter: systems of many constituents with strong interactions between them."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#ContinuumSubstance",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#Substance"
  } ],
  "http://www.w3.org/2002/07/owl#disjointWith" : [ {
    "@id" : "https://w3id.org/emmo/hume#MesoscopicSubstance"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A state that is a collection of sufficiently large number of other parts such that:\n- it is the bearer of qualities that can exists only by the fact that it is a sum of parts\n- the smallest partition dV of the state volume in which we are interested in, contains enough parts to be statistically consistent: n [#/m3] x dV [m3] >> 1"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "ContinuumSubstance"
  } ],
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@language" : "en",
    "@value" : "A continuum is made of a sufficient number of parts that it continues to exists as continuum individual even after the loss of one of them i.e. a continuum is a redundant."
  }, {
    "@language" : "en",
    "@value" : "A continuum is not necessarily small (i.e. composed by the minimum amount of sates to fulfill the definition).\n\nA single continuum individual can be the whole fluid in a pipe."
  }, {
    "@language" : "en",
    "@value" : "A continuum is the bearer of properties that are generated by the interactions of parts such as viscosity and thermal or electrical conductivity."
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#CrystallineMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByStructure"
  }, {
    "@id" : "https://w3id.org/emmo/hume#Solid"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A solid where atoms, ions, or molecules are arranged in a highly ordered, repeating, three-dimensional pattern, forming a crystal lattice that extends throughout the substance, giving it characteristic geometric shapes and distinct physical properties like sharp melting points, unlike amorphous solids with random structures."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "CrystallineMaterial"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#ElementalSubstance",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@language" : "en",
    "@value" : "https://doi.org/10.1351/goldbook.C01022"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#ChemicalSubstance"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "PureSubstance"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A chemical substance composed of atoms with the same number of protons in the atomic nucleus."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "ElementalSubstance"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#Fluid",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#StateOfMatter"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A continuum that has no fixed shape and yields easily to external pressure."
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@language" : "en",
    "@value" : "Gas, liquid, plasma,"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "Fluid"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#Gas",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#Fluid"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Gas is a compressible fluid, a state of matter that has no fixed shape and no fixed volume."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "Gas"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#GranularMaterial",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#MaterialByCompactness"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A granular material is a conglomeration of discrete solid, macroscopic particles that are  large enough to avoid thermal motion fluctuations, and characterized by a loss of energy whenever the particles interact."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "GranularMaterial"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#InorganicCompound",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@language" : "en",
    "@value" : "https://en.wikipedia.org/wiki/Inorganic_compound"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#ChemicalCompound"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A chemical compound that is not organic."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "InorganicCompound"
  } ]
}, {
  "@id" : "https://w3id.org/emmo/hume#IonAtom",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/emmo/hume#StandaloneAtom"
  } ],
  "http://www.w3.org/2002/07/owl#disjointWith" : [ {
    "@id" : "https://w3id.org/emmo/hume#NeutralAtom"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "A standalone atom with an unbalanced number of electrons with respect to its atomic number."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
