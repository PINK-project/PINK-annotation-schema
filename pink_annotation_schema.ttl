#    Classes
#################################################################

### SSbD-related classes
:SsbdDimension rdf:type owl:Class ;
    skos:prefLabel "SsbdDimension"@en ;
    skos:skopeNote "The pink:SsbdDimension is an extension of the 'sustainability dimensions' defined by JRC." ;
    skos:definition "XXX"@en .


### General classes
:Role rdf:type owl:Class ;
    skos:prefLabel "Role"@en ;
    skos:definition "The class of all individuals that are defined through a parthood relation to an entity."@en ;
    vann:usageNote "An individual can be said to have a role in relation to the entity."@en .

:MaterialProcess rdf:type owl:Class ;
    rdfs:subClassOf :Process ;
    skos:prefLabel "MaterialProcess"@en ;
    skos:skopeNote "Since MaterialProcess is a subclass of Process, we say that there must exist a method that describes the material process." ;
    skos:definition "A process that has material as input and output."@en .

:Measurement rdf:type owl:Class ;
    rdfs:subClassOf :Process ;
    skos:prefLabel "Measurement"@en ;
    skos:skopeNote "Since Measurement is a subclass of Process, we say that there must exist a method that describes the measurement." ;
    skos:definition "An observation that results in a quantitative comparison of a 'property' of an 'object' with a standard reference based on a well defined mesurement procedure."@en .

:Computation rdf:type owl:Class ;
    rdfs:subClassOf :Process ;
    skos:prefLabel "Computation"@en ;
    skos:skopeNote "Since Computation is a subclass of Process, we say that there must exist a method that describes the computation." ;
    skos:definition "A procedure that deals with quantitative symbols (i.e. symbols associated with a quantitative oriented language)."@en .


:Method rdf:type owl:Class ;
    rdfs:subClassOf :Data ;
    skos:prefLabel "Method"@en ;
    skos:altLabel "MethodImplementation"@en ;
    skos:definition "Data that describes how to perform an activity."@en .

:ComputationalTool rdf:type owl:Class ;
    rdfs:subClassOf :Method ;
    skos:prefLabel "ComputationalTool"@en ;
    skos:altLabel "ComputationalMethod"@en ;
    skos:definition "A tool that describes how to run a computational model."@en ;
    skos:example "Gromacs is a computational tool that solves Newton's equations of motion."@en .

:Model rdf:type owl:Class ;
    rdfs:subClassOf prov:Entity ;
    skos:prefLabel "Model"@en ;
    skos:definition "A useful simplified description of a selected part of the world."@en ;
    skos:example """Newton's law of motion is a model for how a body will move when subjected to forces.
A mouse can be seen as a model for a human (e.g. in medicine).
An architectural floor plan is a model of a house.
"""@en .



:Process rdf:type owl:Class ;
    rdfs:subClassOf prov:Activity, [
        rdf:type owl:Restriction ;
        owl:onProperty [ owl:inverseOf :hasProcess ] ;
        owl:someValuesFrom :Method
    ] ;
    skos:prefLabel "Process"@en ;
    skos:definition "An activity that is described by a method."@en .


:Material rdf:type owl:Class ;
    rdfs:subClassOf prov:Entity ;
    skos:prefLabel "Material"@en ;
    skos:definition "The class of individuals standing for an amount of matter (ordinary, anti or hybrid) substance (or mixture of substances) in different states of matter or phases."@en .

:Property rdf:type owl:Class ;
    rdfs:subClassOf prov:Entity ;
    skos:prefLabel "Property"@en ;
    skos:definition "A coventional semiotic sigh that stands for an physical interaction with an object using an atomic symbol."@en ;
    vann:usageNote "A property is atomic in the sense that is aimed to deliver one and one only aspect of the object."@en .

:Data rdf:type owl:Class ;
    rdfs:subClassOf prov:Entity ;
    skos:prefLabel "Data"@en ;
    skos:definition "Contrast from variation of properties that is encoded by an agent and that can be decoded by another agent according to a specific rule."@en .

:Object rdf:type owl:Class ;
    rdfs:subClassOf prov:Entity ;
    skos:prefLabel "Object"@en ;
    skos:definition "The class of all individuals who's temporal parts are of the same type as the whole."@en .

:Datum rdf:type owl:Class ;
    rdfs:subClassOf :Data ;
    skos:prefLabel "Datum"@en ;
    skos:definition "A self-consistent encoded data entity."@en ;
    vann:usageNote "Datum is the building block of datasets."@en .

:ReliabilityDocument rdf:type owl:Class ;
    rdfs:subClassOf foaf:Document ;
    skos:prefLabel "ReliabilityDocument"@en ;
    skos:definition "xxx"@en ;
    vann:usageNote "xxx"@en .

:Participant rdf:type owl:Class ;
    rdfs:subClassOf :Object, prov:Agent ;
    skos:prefLabel "Participant"@en ;
    skos:definition "An object which is a spatial part of a process."@en ;
    vann:usageNote "Example: A student during an examination. The student exists both before and after the examination."@en .

:Dataset rdf:type owl:Class ;
    rdfs:subClassOf dcat:Dataset, :Data, [
        rdf:type owl:Restriction ;
        owl:onProperty :hasPart ;
        owl:someValuesFrom :Datum
    ] ;
    skos:prefLabel "Dataset"@en ;
    skos:definition "Encoded data made of more than one datum."@en ;
    vann:usageNote "A collection of (more than one) datum, published or curated by a single agent, and available for access or download in one or more representations."@en .


### Include standard concepts into the PINK taxonomy
