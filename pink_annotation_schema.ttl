###  https://w3id.org/pink#conformance
:conformance rdf:type owl:AnnotationProperty ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:oneOf ( :mandatory
                                      :optional
                                      :recommended
                                    )
                        ] ;
             dcterms:description "Whether the annotation is mandatory, recommended or optional as part of the PINK Annotation Schema"@en ;
             vann:usageNote "It MUST take one of the values 'mandatory', 'recommended' or 'optional'."@en ;
             rdfs:isDefinedBy <https://w3id.org/pink> ;
             skos:prefLabel "conformance" .


