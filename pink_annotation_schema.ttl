:hasAPI a owl:DatatypeProperty ;
    rdfs:domain :Software ;
    rdfs:range xsd:boolean ;
    skos:prefLabel "hasAPI"@en ;
    skos:definition "Whether the software has an API." .

:hasGUI a owl:DatatypeProperty ;
    rdfs:domain :Software ;
    rdfs:range xsd:boolean ;
    skos:prefLabel "hasGUI"@en ;
    skos:definition "Whether the software has a GUI." .


